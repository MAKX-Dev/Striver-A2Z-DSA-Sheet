#include<bits/stdc++.h>
using namespace std;
int longestSubarray(vector<int> nums,int k){
    int n=nums.size();
    int left=0;
    int right=0;
    long long sum=nums[0];
    int maxlen=0;
    while(right<n){
        right++;
        sum+=nums[right];
        while(sum>k && left<=right){
            sum-=nums[left];
            left++;
        }
        if(sum==k){
            maxlen=max(maxlen,right-left+1);
        }
        right++;
        if(right<n){
            sum+=nums[right];
        }
    }
    return maxlen;
}
int main(){
    vector<int> nums={2, 3, 5, 1, 9};
    int k=10;
    int result=longestSubarray(nums,k);
    cout<<"Longest subarray length is: "<<result<<endl;
    return 0;
    

}